global
    log /dev/log local0
    log /dev/log local1 notice
    daemon

defaults
    log global
    mode tcp
    option tcplogi
    option dontlognull
    timeout connect 10s
    timeout client 1m
    timeout server 1m

frontend ssl_frontend
    bind *:443 ssl crt /etc/haproxy/certs/cert.pem
    mode tcp
    option tcplog
    option forwardfor
    default_backend backend_servers

backend backend_servers
    mode tcp
    balance roundrobin
    server server1 23.20.180.164:80 check
    server server2 3.91.191.194:80 check
